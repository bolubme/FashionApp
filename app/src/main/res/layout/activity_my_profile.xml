<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MyProfile">

    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnmenu"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_181pxv"
        android:layout_marginTop="@dimen/_6pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_52">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStylewhite_A700">
            <LinearLayout android:id="@+id/linearRowmenu"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_11pxv"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageMenu"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_menu_15x23"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_menu_15x23"/>
                <LinearLayout android:id="@+id/linearRowlanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_56pxh"
                    android:layout_weight="0.92"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLanguage"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_weight="0.82"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_thrift_fashion"
                        style="@style/txtTenorSans20" tools:text="@string/lbl_thrift_fashion2"/>
                    <ImageView android:id="@+id/imageSearch"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_search_gray_902"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_search_gray_902"/>
                    <ImageView android:id="@+id/imageBag"
                        android:layout_height="@dimen/_21pxh"
                        android:layout_marginStart="@dimen/_21pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_19pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_bag_21x19"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_bag_21x19"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>

        <LinearLayout
            android:id="@+id/linearColumnheadline"
            style="@style/groupStylegray_52"
            android:layout_width="match_parent"
            android:layout_height="617dp"
            android:layout_marginTop="@dimen/_45pxv"
            android:layout_marginEnd="@dimen/_5pxh"
            android:orientation="vertical">

            <TextView
                android:id="@+id/txtHeadline"
                style="@style/txtArimoromanbold34"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginEnd="@dimen/_14pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@string/lbl_my_profile"
                tools:text="@string/lbl_my_profile" />

            <LinearLayout
                android:id="@+id/linearRowtmebayo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_17pxh"
                android:gravity="start"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageImage"
                    android:layout_width="@dimen/_64pxh"
                    android:layout_height="@dimen/_64pxh"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/img_image"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_image" />

                <LinearLayout
                    android:id="@+id/linearColumntmebayo"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_weight="0.78"
                    android:gravity="start"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtTmeBayo"
                        style="@style/txtAlikeAngularregular18"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:ellipsize="end"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_tme_bayo"
                        tools:text="@string/lbl_tme_bayo" />

                    <LinearLayout
                        android:id="@+id/linearRowemail"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/txtEmail"
                            style="@style/txtActorregular14"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_14pxv"
                            android:layout_weight="0.84"
                            android:ellipsize="end"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@string/msg_tme12345_gmail"
                            tools:text="@string/msg_tme12345_gmail" />

                        <TextView
                            android:id="@+id/txtEdit"
                            style="@style/txtAlikeAngularregular15"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_71pxh"
                            android:layout_marginTop="@dimen/_14pxv"
                            android:layout_weight="0.16"
                            android:ellipsize="end"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_edit"
                            tools:text="@string/lbl_edit" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLine3"
                style="@style/groupStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_25pxv"
                android:layout_marginEnd="@dimen/_3pxh"
                android:gravity="center_horizontal"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/linearRowlanguageTwo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txtLanguageTwo"
                        style="@style/txtAbrilFatfaceregular19"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.95"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_create_account3"
                        tools:text="@string/lbl_create_account3" />

                    <ImageView
                        android:id="@+id/imageArrowrightOne"
                        android:layout_width="@dimen/_7pxh"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_201pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright_black_900"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_arrowright_black_900" />
                </LinearLayout>

            </LinearLayout>


                <View
                    android:id="@+id/lineDivider"
                    android:layout_width="@dimen/_375pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_gravity="start"
                    android:layout_marginTop="@dimen/_29pxv"
                    android:background="@color/black_900_5e" />
        </LinearLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>