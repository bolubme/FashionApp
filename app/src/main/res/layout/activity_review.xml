<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Review">

    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnornament"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_82pxv"
        android:layout_marginStart="@dimen/_2pxh"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearReview"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:gravity="start"
                    android:id="@+id/frameStackornament"
                    android:layout_height="@dimen/_483pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageOrnament"
                        android:layout_gravity="top|end"
                        android:layout_height="@dimen/_450pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="@dimen/_223pxh"
                        android:scaleType="fitXY"
                        app:srcCompat="@drawable/img_ornament"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ornament"/>
                    <LinearLayout android:id="@+id/linearHeader"
                        android:layout_gravity="top|end"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_44pxv"
                        android:layout_marginEnd="@dimen/_34pxh"
                        android:layout_marginStart="@dimen/_34pxh"
                        android:layout_marginTop="@dimen/_70pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtText"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_12pxv"
                            android:layout_marginTop="@dimen/_23pxv"
                            android:layout_weight="0.62"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@string/lbl_feedback"
                            style="@style/txtZenKakuGothicAntiquebold24" tools:text="@string/lbl_feedback"/>
                        <ImageButton
                            android:background="@drawable/rectangle_bg_gray_100_radius_30"
                            android:id="@+id/btnButtonCircle"
                            android:layout_height="@dimen/_60pxh"
                            android:layout_marginStart="@dimen/_49pxh"
                            android:layout_width="@dimen/_60pxh"
                            android:paddingHorizontal="@dimen/_20pxh"
                            android:paddingVertical="@dimen/_20pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_buttoncircle" tools:ignore="ContentDescription"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHeadlineOne"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_164pxv"
                        android:layout_marginEnd="@dimen/_109pxh"
                        android:layout_marginStart="@dimen/_109pxh"
                        android:layout_marginTop="@dimen/_164pxv"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@string/msg_what_is_you_rat"
                        style="@style/txtMetropolissemibold18" tools:text="@string/msg_what_is_you_rat"/>
                    <LinearLayout android:id="@+id/linearRowstar"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_203pxv"
                        android:layout_marginEnd="@dimen/_52pxh"
                        android:layout_marginStart="@dimen/_52pxh"
                        android:layout_marginTop="@dimen/_203pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageStar"
                            android:layout_height="@dimen/_34pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_width="@dimen/_36pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_star"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                        <ImageView android:id="@+id/imageStarOne"
                            android:layout_height="@dimen/_34pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_width="@dimen/_36pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_star"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                        <ImageView android:id="@+id/imageStarTwo"
                            android:layout_height="@dimen/_34pxh"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_36pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_star"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                        <ImageView android:id="@+id/imageStarThree"
                            android:layout_height="@dimen/_34pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_width="@dimen/_36pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_star"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                        <ImageView android:id="@+id/imageStarFour"
                            android:layout_height="@dimen/_34pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_width="@dimen/_36pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_star"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearReview1"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:gravity="center_horizontal"
                            android:id="@+id/txtHeadlineTwo"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_77pxh"
                            android:layout_marginStart="@dimen/_81pxh"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_4pxh"
                            android:text="@string/msg_please_share_yo"
                            style="@style/txtMetropolissemibold18" tools:text="@string/msg_please_share_yo"/>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumnyourreview"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_18pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                            <EditText android:ellipsize="end"
                                android:gravity="start"
                                android:id="@+id/txtYourreview"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_124pxv"
                                android:layout_marginEnd="@dimen/_16pxh"
                                android:layout_marginStart="@dimen/_16pxh"
                                android:layout_marginTop="@dimen/_15pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@string/lbl_your_review"
                                style="@style/txtMetropolisregular14" tools:text="@string/lbl_your_review"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearButtonSquare"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_27pxh"
                    android:layout_marginStart="@dimen/_29pxh"
                    android:layout_marginTop="@dimen/_87pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStyleamber_600cornerRadius">

                    <TextView
                        android:id="@+id/txtLanguage"
                        style="@style/txtPoppinsmedium18"
                        android:layout_width="86dp"
                        android:layout_height="28dp"
                        android:layout_marginStart="@dimen/_131pxh"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_marginBottom="@dimen/_21pxv"
                        android:layout_weight="0.92"
                        android:ellipsize="end"
                        android:singleLine="false"
                        android:text="@string/lbl_submit"
                        tools:text="@string/lbl_submit" />
                    <ImageView android:id="@+id/imageArrowright"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_24pxv"
                        android:layout_marginEnd="@dimen/_30pxh"
                        android:layout_marginStart="@dimen/_96pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="@dimen/_5pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright_white_a700"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_white_a700"/>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout></ScrollView>