<?xml version="1.0" encoding="utf-8"?>

<ScrollView android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".cart">

    <LinearLayout android:id="@+id/linearCart1"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageClose"
            android:layout_gravity="start"
            android:layout_height="@dimen/_12pxh"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="@dimen/_12pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_close_13x13"
            tools:ignore="ContentDescription" tools:src="@drawable/img_close_13x13"/>
        <TextView android:ellipsize="end" android:id="@+id/txtCART"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_marginTop="@dimen/_23pxv"
            android:layout_width="wrap_content"
            android:letterSpacing="0.2"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:singleLine="true"
            android:text="CART"
            android:textAllCaps="true" style="@style/txtTenorSans18_4" tools:text="@string/lbl_cart2"/>

<!--            First one -->
        <LinearLayout android:id="@+id/linearCart2"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_margin="@dimen/_20pxv"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageRectangle3441"
                android:layout_height="@dimen/_135pxh"
                android:layout_width="@dimen/_101pxh"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/img_rectangle344_9"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle344_9"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnlamerei1"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_marginStart="@dimen/_32pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_weight="0.49" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLamerei1"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.1"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="H X O"
                    android:textAllCaps="true"
                    style="@style/txtTenorSans14_8" tools:text="@string/lbl_h_x_o"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtRecycleBoucle1"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="Denim Jean Bag"
                    style="@style/txtTenorSans12_3" tools:text="@string/lbl_denim_jean_bag"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowone1"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <RadioButton android:buttonTint="@color/gray_400_6c"
                        android:ellipsize="end"
                        android:fontFamily="@font/tenor_sans"
                        android:id="@+id/rbOne1"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.64"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:paddingBottom="@dimen/_4pxv"
                        android:paddingStart="@dimen/_13pxh"
                        android:paddingTop="@dimen/_4pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_1"
                        android:textColor="@color/black_900"
                        android:textFontWeight="400"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:text="@string/lbl_1"/>
                    <ImageButton
                        android:background="@drawable/rectangle_border_gray_400_6c_radius_12_17"
                        android:id="@+id/btnCloseOne1"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:paddingHorizontal="@dimen/_6pxh"
                        android:paddingVertical="@dimen/_6pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_close" tools:ignore="ContentDescription"/>
                </LinearLayout>
                <TextView android:ellipsize="end" android:id="@+id/txtPrice1"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text=""
                    style="@style/txtTenorSans15" tools:text="@string/lbl_52"/>
            </LinearLayout>
        </LinearLayout>

<!--        Second One -->
        <LinearLayout android:id="@+id/linearCart"
            android:layout_marginLeft="@dimen/_20pxv"
            android:layout_marginRight="@dimen/_20pxv"
            android:layout_marginTop="0dp"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageRectangle344"
                android:layout_width="@dimen/_101pxh"
                android:layout_height="@dimen/_135pxh"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/img_rectangle344_220x165"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_rectangle344_9" />
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnlamerei"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_marginStart="@dimen/_32pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_weight="0.49" android:layout_width="0dp" android:orientation="vertical">

                <TextView
                    android:id="@+id/txtLamerei"
                    style="@style/txtTenorSans14_8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:ellipsize="end"
                    android:letterSpacing="0.1"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@string/_21_8_x_67"
                    android:textAllCaps="true"
                    tools:text="@string/lbl_21_8_x_67" />

                <TextView
                    android:id="@+id/txtRecycleBoucle"
                    style="@style/txtTenorSans12_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@string/msg_womens_grey_cot"
                    tools:text="@string/msg_womens_grey_cot" />
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowone"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <RadioButton android:buttonTint="@color/gray_400_6c"
                        android:ellipsize="end"
                        android:fontFamily="@font/tenor_sans"
                        android:id="@+id/rbOne"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.64"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:paddingBottom="@dimen/_4pxv"
                        android:paddingStart="@dimen/_13pxh"
                        android:paddingTop="@dimen/_4pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_1"
                        android:textColor="@color/black_900"
                        android:textFontWeight="400"
                        android:textSize="@dimen/_14pxh"
                        android:textStyle="bold" tools:text="@string/lbl_1"/>
                    <ImageButton
                        android:background="@drawable/rectangle_border_gray_400_6c_radius_12_17"
                        android:id="@+id/btnCloseOne"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:paddingHorizontal="@dimen/_6pxh"
                        android:paddingVertical="@dimen/_6pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_close" tools:ignore="ContentDescription"/>
                </LinearLayout>
                <TextView android:ellipsize="end" android:id="@+id/txtPrice"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text=""
                    style="@style/txtTenorSans15" tools:text="@string/lbl_52"/>
            </LinearLayout>
        </LinearLayout>


        <View android:background="@color/black_900_6c"
            android:id="@+id/lineLineNineteen"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_90pxv" android:layout_width="@dimen/_347pxh"/>
        <LinearLayout android:id="@+id/linearRowsubtotal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_marginBottom="@dimen/_15pxh"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtSubTotal"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_weight="0.76" android:layout_width="0dp"
                android:letterSpacing="0.1"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@string/lbl_sub_total"
                android:textAllCaps="true" style="@style/txtTenorSans14" tools:text="@string/lbl_sub_total"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPriceTwo"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_212pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.24" android:layout_width="0dp"
                android:letterSpacing="0.15"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@string/lbl_15"
                android:textAllCaps="true"
                style="@style/txtTenorSans16_7" tools:text="@string/lbl_15"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearContent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_4pxh"
            android:layout_marginTop="0pt"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStyleamber_600">
            <ImageView android:id="@+id/imageBag"
                android:layout_height="@dimen/_19pxh"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginStart="@dimen/_127pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="@dimen/_16pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_bag_19x16"
                tools:ignore="ContentDescription" tools:src="@drawable/img_bag_19x16"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtCheckout"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_18pxv"
                android:layout_marginEnd="@dimen/_125pxh"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_weight="0.84" android:layout_width="0dp"
                android:letterSpacing="0.01"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="BUY NOW"
                android:textAllCaps="true"
                style="@style/txtTenorSans16_1" tools:text="@string/lbl_buy_now"/>
        </LinearLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>