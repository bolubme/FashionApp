<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu">

    <FrameLayout android:id="@+id/frameMenu"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnclose"
            android:layout_gravity="top|start"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_16pxv"
            android:layout_marginEnd="@dimen/_19pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowclose"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageClose"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_width="@dimen/_19pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_close_13x13"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_close_13x13"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTHRIFTFASHION"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_62pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_weight="0.94"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_thrift_fashion"
                        style="@style/txtTenorSans20_2" tools:text="@string/lbl_thrift_fashion"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowwomen"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtWomen"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.46"
                    android:layout_width="0dp"
                    android:letterSpacing="0.15"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_women"
                    android:textAllCaps="true"
                    style="@style/txtTenorSans14_5" tools:text="@string/lbl_women"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMan"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_54pxh"
                    android:layout_weight="0.27"
                    android:layout_width="0dp"
                    android:letterSpacing="0.15"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_man"
                    android:textAllCaps="true"
                    style="@style/txtTenorSans14_7" tools:text="@string/lbl_man"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtKids"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_57pxh"
                    android:layout_weight="0.28"
                    android:layout_width="0dp"
                    android:letterSpacing="0.15"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_kids"
                    android:textAllCaps="true"
                    style="@style/txtTenorSans14_7" tools:text="@string/lbl_kids"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameStacklinethirteen"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_8pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_309pxh">
                <View android:background="@color/bluegray_400_6c"
                    android:id="@+id/lineLineThirteen"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_244pxh"/>
                <View android:background="@color/red_300"
                    android:id="@+id/lineLineTwelve"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_71pxh"/>
                <ImageView android:id="@+id/imageOffer"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_8pxh"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_width="@dimen/_8pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_offer"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_offer"/>
            </FrameLayout>
            <LinearLayout android:id="@+id/linearRownewapparelbag"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_56pxv"
                android:layout_width="match_parent" android:orientation="horizontal">

                <TextView
                    android:id="@+id/txtNewApparelBag"
                    style="@style/txtTenorSans16_4"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.81"
                    android:lineSpacingExtra="@dimen/_32pxh"
                    android:text="@string/msg_new_apparel_bag"
                    tools:text="@string/msg_new_apparel_bag" />
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearDropdown"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_249pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.19"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageArrowdown"
                        android:layout_height="@dimen/_6pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowdown"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowdown"/>
                    <ImageView android:id="@+id/imageArrowdownOne"
                        android:layout_height="@dimen/_6pxh"
                        android:layout_marginTop="@dimen/_42pxv"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowdown"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowdown"/>
                    <ImageView android:id="@+id/imageArrowdownTwo"
                        android:layout_height="@dimen/_6pxh"
                        android:layout_marginTop="@dimen/_42pxv"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowdown"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowdown"/>
                    <ImageView android:id="@+id/imageArrowdownThree"
                        android:layout_height="@dimen/_6pxh"
                        android:layout_marginTop="@dimen/_42pxv"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowdown"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowdown"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearStorelocator"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_35pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageUser"
                    android:layout_height="@dimen/_22pxh"
                    android:layout_width="@dimen/_22pxh"


                    android:scaleType="fitXY"
                    android:src="@drawable/img_user"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtRegisterLogin"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_weight="0.88"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_register_login"
                    style="@style/txtTenorSans16_3" tools:text="@string/lbl_register_login"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearStorelocator1"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageSettings"
                    android:layout_width="@dimen/_29pxh"
                    android:layout_height="@dimen/_27pxh"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_marginBottom="@dimen/_2pxv"

                    android:scaleType="fitXY"
                    android:src="@drawable/img_settings"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_settings" />

                <TextView android:ellipsize="end"
                    android:id="@+id/txtSettingsOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_11pxh"

                    android:layout_weight="0.76"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_settings"
                    style="@style/txtTenorSans16_3" tools:text="@string/lbl_settings"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearStorelocator2"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageStar"
                    android:layout_height="@dimen/_22pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_star_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_star_black_900"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFeedback"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_weight="0.77"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_feedback"
                    style="@style/txtTenorSans16_3" tools:text="@string/lbl_feed_back2"/>
            </LinearLayout>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRowhome"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_8pxh"
                android:layout_marginStart="@dimen/_32pxh"
                android:layout_marginTop="@dimen/_42pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtHome"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.3"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_home"
                    style="@style/txtTenorSans16_6" tools:text="@string/lbl_home"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAbout"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_63pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.3"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_about"
                    style="@style/txtTenorSans16_6" tools:text="@string/lbl_about"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtContact"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_56pxh"
                    android:layout_weight="0.4"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_contact"
                    style="@style/txtTenorSans16_6" tools:text="@string/lbl_contact"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageDivider"
                android:layout_gravity="end"
                android:layout_height="@dimen/_9pxh"
                android:layout_marginEnd="@dimen/_88pxh"
                android:layout_marginStart="@dimen/_88pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="@dimen/_124pxh"
                android:scaleType="fitXY" android:src="@drawable/img_8"
                tools:ignore="ContentDescription" tools:src="@drawable/img_8"/>
            <ImageView android:id="@+id/imageSocial"
                android:layout_gravity="end"
                android:layout_height="@dimen/_19pxh"
                android:layout_marginEnd="@dimen/_77pxh"
                android:layout_marginStart="@dimen/_77pxh"
                android:layout_marginTop="@dimen/_26pxv"
                android:layout_width="@dimen/_141pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_social"
                tools:ignore="ContentDescription" tools:src="@drawable/img_social"/>
        </LinearLayout>
    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>